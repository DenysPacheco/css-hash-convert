{
    "filesSearch": [
        ".html",
        ".css"
    ],
    "filesIgnore": [
        "-hashed"
    ],
    "dirsIgnore": [
        ".",
        "packages",
        "src",
        "dist"
    ],
    "sufix": "-hashed",
    "prefix": "_",
    "patternCSS": "[\\.\\#]-?([_a-zA-Z]+[_a-zA-Z0-9-]*)\\s*\\{",
    "patternHTML": "class[\t]*=[\t]*\"[^\"]+",
    "hashLength": 6,
    "overwriteFiles": true,
    "minimize": true,
    "console": true
}